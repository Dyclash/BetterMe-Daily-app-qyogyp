
{
  "expo": {
    "name": "BetterMe Daily",
    "slug": "BetterMe Daily",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/d3e3b13e-9a90-4c24-b59f-751d92e5ce19.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/d3e3b13e-9a90-4c24-b59f-751d92e5ce19.png",
      "resizeMode": "contain",
      "backgroundColor": "#FF5A5F"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.BetterMeDaily",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/d3e3b13e-9a90-4c24-b59f-751d92e5ce19.png",
        "backgroundColor": "#FF5A5F"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.bettermedaily"
    },
    "web": {
      "favicon": "./assets/images/d3e3b13e-9a90-4c24-b59f-751d92e5ce19.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-font",
      "expo-router",
      "expo-web-browser"
    ],
    "scheme": "bettermedaily",
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {}
    }
  },
  "scheme": "BetterMe Daily"
}
